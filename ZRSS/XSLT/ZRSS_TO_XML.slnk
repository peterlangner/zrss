<?xml version="1.0" encoding="utf-8"?>
<XSLT XSLTDESC="ZRSS_TO_XML" DEVCLASS="$TMP" LANGU="D" DESCRIPT="Return the content of a RSS feed as a XML file">
 <source>&lt;?sap.transform simple?&gt;
