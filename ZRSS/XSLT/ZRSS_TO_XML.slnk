&lt;?sap.transform simple?&gt;
&lt;?sap.transform simple?&gt;
&lt;tt:transform xmlns:tt=&quot;http://www.sap.com/transformation-templates&quot;&gt;
  &lt;tt:root name=&quot;RSSFEED&quot;/&gt;
  &lt;tt:template&gt;
    &lt;rss version=&quot;2.0&quot;&gt;
      &lt;channel&gt;
        &lt;title&gt;
          &lt;tt:value ref=&quot;RSSFEED.TITLE&quot;/&gt;
        &lt;/title&gt;
        &lt;description&gt;
          &lt;tt:value ref=&quot;RSSFEED.DESCRIPTION&quot;/&gt;
        &lt;/description&gt;
        &lt;link&gt;
          &lt;tt:value ref=&quot;RSSFEED.LINK&quot;/&gt;
        &lt;/link&gt;
        &lt;language&gt;
          &lt;tt:value ref=&quot;RSSFEED.LANGU&quot;/&gt;
        &lt;/language&gt;
        &lt;image&gt;
          &lt;url&gt;
            &lt;tt:value ref=&quot;RSSFEED.URL_I&quot;/&gt;
          &lt;/url&gt;
          &lt;title&gt;
            &lt;tt:value ref=&quot;RSSFEED.TITLE_I&quot;/&gt;
          &lt;/title&gt;
          &lt;link&gt;
            &lt;tt:value ref=&quot;RSSFEED.LINK_I&quot;/&gt;
          &lt;/link&gt;
        &lt;/image&gt;
        &lt;tt:loop ref=&quot;RSSFEED.ITEMS&quot;&gt;
          &lt;item&gt;
            &lt;title&gt;
              &lt;tt:value ref=&quot;TITLE&quot;/&gt;
            &lt;/title&gt;
            &lt;link&gt;
              &lt;tt:value ref=&quot;LINK&quot;/&gt;
            &lt;/link&gt;
            &lt;description&gt;
              &lt;tt:value ref=&quot;DESCRIPTION&quot;/&gt;
            &lt;/description&gt;
            &lt;author&gt;
              &lt;tt:value ref=&quot;AUTHOR&quot;/&gt;
            &lt;/author&gt;
            &lt;pubdate&gt;
              &lt;tt:value ref=&quot;PUBDATE&quot;/&gt;
            &lt;/pubdate&gt;
            &lt;guid&gt;
              &lt;tt:value ref=&quot;GUID&quot;/&gt;
            &lt;/guid&gt;
          &lt;/item&gt;
        &lt;/tt:loop&gt;
      &lt;/channel&gt;
    &lt;/rss&gt;
  &lt;/tt:template&gt;
&lt;/tt:transform&gt;</source>
</XSLT>
